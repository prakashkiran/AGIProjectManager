<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.welcome.agiprojectmanager">

    <uses-sdk
        android:minSdkVersion="19"
        android:targetSdkVersion="25" />

    <meta-data
        android:name="my.app.namespace.utils.GlideConfiguration"
        android:value="GlideModule" />

    <uses-feature
        android:name="android.hardware.sensor.compass"
        android:required="true" />

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/apm"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.NoActionBar">
        <activity android:name=".Home" />
        <activity android:name=".Login" />
        <activity android:name=".Registration" />
        <activity android:name=".Departments" />
        <activity android:name=".Projects" />
        <activity android:name=".MiniProject" />
        <activity android:name=".Upload" />
        <activity android:name=".ViewProjects" />
        <activity android:name=".splashscreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".listofprojects" />
        <activity android:name=".Listofprojects" />
        <activity android:name=".lsp1" />
        <activity android:name=".lsp2"></activity>
        <activity android:name=".lsp1" />
        <activity android:name=".lsp1" />
    </application>

</manifest>